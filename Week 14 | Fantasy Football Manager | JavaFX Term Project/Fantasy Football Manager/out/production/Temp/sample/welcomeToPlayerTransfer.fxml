<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="488.0" prefWidth="679.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.welcomeToPlayerTransferController">
   <children>
      <Label layoutX="76.0" layoutY="182.0" text="Welcome To Player Transfer" textFill="#2c2c2a">
         <font>
            <Font name="System Bold Italic" size="42.0" />
         </font>
      </Label>
      <ImageView fitHeight="141.0" fitWidth="200.0" layoutX="-3.0">
         <image>
            <Image url="@football.jpg" />
         </image>
      </ImageView>
      <Button fx:id="backButton" layoutX="59.0" layoutY="396.0" mnemonicParsing="false" onAction="#goBack" prefHeight="25.0" prefWidth="76.0" text="Back">
         <font>
            <Font name="Candara" size="22.0" />
         </font>
      </Button>
      <Button fx:id="logintoTransferButton" layoutX="263.0" layoutY="277.0" mnemonicParsing="false" onAction="#loginTransferAttempt" prefHeight="42.0" prefWidth="173.0" text="Enter Market">
         <font>
            <Font name="Arial" size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
